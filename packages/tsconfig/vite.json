{
   "$schema": "https://json.schemastore.org/tsconfig",
   "extends": "./base.json",
   "Display": "Vite",
   "compilerOptions": {
      "allowJs": true,
      "esModuleInterop": true,
      "declaration": false,
      "declarationMap": false,
      "incremental": true,
      "jsx": "preserve",
      "lib": ["dom", "dom.iterable", "esnext"],
      "module": "CommonJS",
      "noEmit": true,
      "resolveJsonModule": true,
      "strict": false,
      "target": "es5",
      "allowImportingTsExtensions": true
   }
}
